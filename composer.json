        "typo3",
        "fe",
        "chat"
    ],
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Sjoerd Zonneveld",
            "homepage": "https://www.bitpatroon.nl",
            "role": "Developer",
            "email": "code@bitpatroon.nl"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "bitpatroon/bpn_configuration": "^10.4"
    },
    "config": {
        "optimize-autoloader": true
    },
